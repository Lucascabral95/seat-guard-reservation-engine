generator client {
  // provider     = "prisma-client"
  // output       = "../generated/prisma"
  // moduleFormat = "cjs"
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
}